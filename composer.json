{
    "name": "krivov/yahoo-finance-api",
    "type": "library",
    "description": "PHP library for accessing Yahoo Finance data",
    "keywords": ["yahoo", "finance", "stock", "api"],
    "homepage": "https://github.com/krivov/yahoo-finance-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Krivov",
            "email": "sergey@krivov.info"
        },
        {
            "name": "Christian Scheb",
            "email": "me@christianscheb.de"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5"
    },
    "autoload": {
        "psr-4": {
            "Scheb\\YahooFinanceApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scheb\\YahooFinanceApi\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
